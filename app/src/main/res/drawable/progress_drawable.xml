<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <stroke
                android:color="@color/white"
                android:width="@dimen/dp2"/>
            <corners android:radius="@dimen/dp10"/>
            <solid android:color="@color/background_color"/>
        </shape>
    </item>

    <item android:id="@android:id/progress" android:left="@dimen/dp3"
        android:right="@dimen/dp3"
        android:top="@dimen/dp3"
        android:bottom="@dimen/dp3">

        <clip>
            <shape>

                <stroke
                    android:color="#EBAA41"
                    android:width="@dimen/dp2"/>
                <corners android:radius="@dimen/dp10"/>
                <solid android:color="@color/yellow"/>
            </shape>
        </clip>

    </item>


</layer-list>